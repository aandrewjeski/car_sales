<div>
  <h4> Cars </h4>
  <table style="width:400px">
    <tr>
      <th>Model</th>
      <th>Manufacturer</th>
      <th>Color</th>
      <th>Year</th>
      <th>Mileage</th>
      <th>
    </tr>
    <tr>
      <% @cars.each do |car| %>
        <td><%= car.name %></td>
        <td><%= car.manufacturer %></td>
        <td><%= car.color %></td>
        <td><%= car.year %></td>
        <td><%= car.mileage %></td>
      <% end %>
    </tr>
  </table>
</div>
<%= link_to 'Add a Car', new_car_path(@car) %>
